datasource db {
  provider = "postgresql"
  url= env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  resume    String
  techStack String
  createdAt DateTime @default(now())
}

model Job {
  id            Int      @id @default(autoincrement())
  title         String
  description   String
  requiredSkills String
  createdAt     DateTime @default(now())
}
